{% for proc in Processes %}

{{proc.name|capitalize}}({{proc.variable}}) =
    let
        {% for calc in calculations %}
        {{calc}}
        {% endfor %}
    within
        {% for channel in channels %}
        {% for y in communications_variable %}
        {{channe.name}} ! {{y}} ->
        {% endfor %}
        {% endfor %}
        SKIP

{% for channel in channels %}
{{channel.assert_proc_name}}(c) =
    c ? x ->
    if {{channel.lowerbound}} <= x and x <= {{channel.upperbound}}
    then SKIP else STOP
{% endfor %}
{% endfor %}