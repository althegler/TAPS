{{proc.name|capitalize}}({{proc.variable}}) =
    let
        {{for x in list_of_calculations:}}
        {{calculation}}
    within
        {{for y in communications}}
        {{channel_name}} ! {{variable_to_send}} ->
        SKIP

{{for c in channel_names}}
{{channel_name|capitalize}}_assert(c) =
    c ? x -> if {{proc.lowerbound}} <= x and x <= {{proc.upperbound}}
    then SKIP else STOP

