proc id (in inbus)
    bus idout {val: u32 range 0 to 10;};
{
    idout.val = inbus.val;
}

proc plusone (in inbus)
    bus plusout {val: u32 range 0 to 10;};
{
    plusout.val = inbus.val + 1;
}

network plusone_net ()
{
    instance id_inst of id(plusone_inst.plusout);
    instance plusone_inst of plusone(id_inst.idout);
}
